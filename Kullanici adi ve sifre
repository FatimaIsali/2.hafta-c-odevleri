using System;

class Program
{
    static void Main()
    {
        Console.WriteLine("Kullanici adini giriniz:");
        string kullaniciadi = Console.ReadLine();

        Console.WriteLine("Sifreyi giriniz:");
        string sifre = Console.ReadLine();

        if (kullaniciadi=="admin"&& sifre=="_32453@")
        {
            Console.WriteLine("Giris basarili.");
        }
        else
        {
            Console.WriteLine("Kullani adi ve ya sifre hatali.");
        }
        
    }
}

